<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Paolo Vigori</Authors>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>Autofac.KeyedDecorators</PackageId>
    <PackageTags>autofac;keyedservices;decorators</PackageTags>
    <Description>Helper functions for registering decorators of keyed services that support Autofac indexes (Autofac.Features.Indexed.IIndex&lt;K,V&gt;)</Description>
    <Copyright>Copyright © 2018 Paolo Vigori</Copyright>
    <PackageLicenseUrl>http://www.opensource.org/licenses/mit-license.php</PackageLicenseUrl>
    <Version>1.0.4</Version>
    <TargetFrameworks>netcoreapp2.0;net45;net47</TargetFrameworks>
    <FileVersion>1.0.4.0</FileVersion>
    <AssemblyVersion>1.0.4.0</AssemblyVersion>
    <PackageProjectUrl>https://github.com/paolov/Autofac.KeyedDecorators</PackageProjectUrl>
    <RepositoryUrl>https://github.com/paolov/Autofac.KeyedDecorators</RepositoryUrl>
    <PackageIconUrl>https://raw.githubusercontent.com/paolov/Autofac.KeyedDecorators/master/icon.png</PackageIconUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Autofac" Version="4.8.1" />
  </ItemGroup>
</Project>